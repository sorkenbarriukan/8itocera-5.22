<theme>
	<formatVersion>4</formatVersion>
	<include>./../theme.xml</include>

<view name="system">
 <image name="background_color" extra="true">
  <path>./.png</path>
  <origin>0 0</origin>
  <pos>0 0</pos>
  <size>1 1</size>
  <zIndex>10</zIndex>
 </image>
</view>

<view name="system, basic, detailed, grid, video">
