 </image>
</view>

<view name="basic, detailed,video">
 <image name="console" extra="true">
  <path>./.png</path>
 <origin>0 0</origin>
  <pos>0 0</pos>
  <size>1 1</size>
 </image>
 
 
  <image name="cart" extra="true">
  <path>./.png</path>
<origin>.5 .5</origin>
<pos>0.6095 0.55</pos>
<size>0.159 0.150</size>
<zIndex>10</zIndex>
 </image>
 
 <image name="md_image">
	<origin>0.5 0.5</origin>
	<pos>0.44 0.46</pos>
	<maxSize>0.25 0.44</maxSize>
</image>

</view>


<feature supported="video">
<view name="video">

<video name="md_video">
<origin>0.5 0.5</origin>
